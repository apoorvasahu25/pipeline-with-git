pipeline {
    agent any
    stages {
        stage('Build') {
            
            steps {
                echo 'Hello,this is a code step'
                
            }
        }
        stage('build') {
            
            steps {
                echo 'Hello,this is build step'
                
            }
        }
        stage('deploy') {
            
            steps {
                echo 'Hello,this is deploy step'
    }
}
}
}
